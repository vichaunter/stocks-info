{
  "name": "stocks-info",
  "version": "1.0.0",
  "description": "The Stocks info api that everybody should have",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"DEV=true ts-node ./test/dev/index.ts\"",
    "start": "ts-node ./src/index.ts",
    "services": "MODE=services yarn start",
    "build": "rimraf dist && yarn tsc",
    "ts:check": "yarn tsc --project tsconfig.json",
    "add-build": "git add dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "sh ./scripts/deploy.sh",
    "db:generate": "yarn prisma generate",
    "db:seed": "node ./database/seed/seed.js",
    "db:update": "node ./database/seed/updateHandlers.js",
    "db:dump": "node ./database/seed/dumpDb.js",
    "db:create": "node ./database/seed/createTickers.js"
  },
  "keywords": [
    "stocks",
    "info"
  ],
  "author": "VicHaunter",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "5.3.1",
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "^8.2.0",
    "csv-parser": "^3.0.0",
    "csv-string": "^4.1.1",
    "csv-writer": "^1.6.0",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.10",
    "dotenv": "16.3.1",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "moment": "^2.29.4",
    "onchange": "^7.1.0",
    "papaparse": "^5.4.1",
    "picocolors": "^1.0.0",
    "puppeteer": "^21.2.1",
    "puppeteer-core": "^21.2.1",
    "system-locale": "^0.1.0",
    "ramda": "^0.29.1"
  },
  "devDependencies": {
    "localtunnel": "^2.0.2",
    "ngrok": "^5.0.0-beta.2",
    "nodemon": "^3.0.1",
    "pre-commit": "^1.2.2",
    "prisma": "^5.3.1",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0-dev.20230917",
    "yargs": "^17.7.2"
  },
  "pre-commit": [
    "ts:check",
    "build",
    "add-build"
  ]
}
